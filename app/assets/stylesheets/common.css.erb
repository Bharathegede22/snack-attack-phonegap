.button, .button-dis, .btn-default, .btn-default:hover, .btn-default:focus, .btn-default:active, .btn-default.active, .btn-notify {
	cursor:pointer;
	color: #fff !important;
	-webkit-font-smoothing: antialiased;
	background: #6ABD19;
  text-shadow:none;
  -webkit-border-radius: 5px;
	-khtml-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
	font-weight:bold;
	behavior: url(<%= asset_path 'assets/PIE.htc' %>);
	border:none;
}

.button, .button-dis, .btn-notify {
	font-size: 18px;
	padding:5px 0;
  text-align:center;
  border:none;
}

.button-dis {
	background:#999999;
	cursor:default;
}

.btn-notify {
	background:#F5A623;
}
.button:hover, .btn-default:hover, .btn-notify {
	text-shadow: 1px 1px 1px #000;
}

/* Table */
