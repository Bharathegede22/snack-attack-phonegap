.footer h3 {
	color:#ffffff;
	margin:5px 0;
	font-weight:bold;
	font-size:14px;
}

.footer .partition {
	width:320px;
	float:left;
}

.footer .social {
	background:url(<%= asset_path 'r.png' %>) no-repeat;
	background-position:0px -170px;
	height:25px;
	width:155px;
	display:block;
	margin-top:10px;
}

.footer .social .link {
	height:25px;
	width:25px;
	margin-left:5px;
	float:left;
	display:block;
}

.footer .logo-rtt {
	margin:5px auto 0 auto;
	float:none;
}

.overlay {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background:url(<%= asset_path 't.png' %>);
	z-index:20000;
	overflow:hidden;
	display:none;
}

* html .overlay {
	background:url(<%= asset_path 'b.gif' %>);
	filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(enabled=true, sizingMethod=scale, src=<%= asset_path 't.png' %>);
}

.calculator-wrapper {
	position:fixed;
	top:50%;
	left:50%;
	z-index:50000;
}

.calculator-wrapper .close {
	background:url(<%= asset_path 'r.png' %>) no-repeat;
	background-position:-260px -170px;
	cursor:pointer;
	width:30px;
	height:30px;
	position:absolute;
	top:-265px;
	right:-235px;
}

.calculator-modal {
	position:absolute;
	top:-250px;
	left:-220px;
	width:960px;
	display:none;
}

* html .calculator-modal {
	position: absolute;
}

.calculator-modal .content {
	background:#fff;
	width:440px;
	padding:0px;
	font-size:14px;
	color:#727272;
	-webkit-box-shadow: 0px 0px 10px #000000;
	-moz-box-shadow: 0px 0px 10px #000000;
	box-shadow: 0px 0px 10px #000000;
	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	border-radius: 10px;
	behavior: url(<%= asset_path 'PIE.htc' %>);
	padding:10px 0;
}

.calculator-modal .content .wait-box {
	padding:100px 0;
	text-align:center;
	color:#666;
	font-size:20px;
}

.calculator-modal .content .title {
	background: #6ABD19;
	padding:5px;
	color:#fff;
	font-size:20px;
	text-align:center;
	margin:0 10px 10px 10px;
	font-weight:bold;
}

.calculator-modal .content .left {
	margin:7px 10px 0 5px;
	float:left;
	width:150px;
	text-align:right;
}

.calculator-modal .content .small {
	width:210px;
	position:relative;
	overflow:visible;
	float:left;
}

.calculator-modal .content .small ul {
	width:100%;
	position:absolute;
	top:28px;
	left:0px;
	background:#e6e6e6;
	display:none;
	z-index:26000;
}

.calculator-modal .content .small ul li {
	padding:5px;
	cursor:pointer;
	font-size:14px;
}

.calculator-modal .content .small ul li:hover {
	background:#727272;
	color:#fff;
}

.calculator-modal .content .input {
	border:2px solid #ccc;
	background:#f6f6f6;
	overflow:hidden;
	position:relative;
	height:24px;
}

.calculator-modal .content .input .arrow {
	background:url(<%= asset_path 'r.png' %>) no-repeat;
	background-position:-260px -140px;
	height:20px;
	width:20px;
	position:absolute;
	top:0px;
	right:0px;
	z-index:1;
}

.calculator-modal .content .input input {
	background:transparent;
	padding:0px;
	border:none;
	font-size:14px;
	width:100%;
	color:#999999;
	position:absolute;
	top:3px;
	left:5px;
	z-index:25000;
}

.calculator-modal .content .ui-datepicker-inline {
