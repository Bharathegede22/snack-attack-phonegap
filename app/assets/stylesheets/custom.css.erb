#indicator-1 { background: #96D05E; }
#indicator-2 { background: url(<%= asset_path 'safe.png' %>); }
#indicator-3 { background: url(<%= asset_path 'unsafe.png' %>); }

.money {
	background: #96D05E;
}

.safe-booking, .unsafe-booking {
	cursor: pointer;
	opacity: 0.8;
	padding: 10px 0;
}
.safe-booking:hover, .unsafe-booking:hover {
	opacity: 0.9;
}
.safe-booking {
	border-left: 20px solid #5CA315;
	border-right: 5px solid #5CA315;
	background: url(<%= asset_path 'safe.png' %>);
}
.unsafe-booking {
	border-left: 20px solid #A31515;
	border-right: 5px solid #A31515;
	background: url(<%= asset_path 'unsafe.png' %>);
}
.wallet-car {
	width: 25px;
	height: 10px;
	position: relative;
	background: url(<%= asset_path 'wallet-car.png' %>);
}

.highlight {
	background: #E2FFC5;
}

.unsafe-booking-entry {
	border-left: 6px solid #A31515;
}

.mobile-app .iphone {background-position: 0px -100px;}
.mobile-app .android {background-position: -20px -100px;}
.mobile-app .iphone:hover {background-position: 0px -125px;}
.mobile-app .android:hover {background-position: -20px -125px;}

#resume_widget {
	bottom: 0;
	left: 30px;
	width: 240px;
	z-index: 100;
}
#resume_widget .time {
	width: 116px;
}
#resume_header {
	color: #FFF;
	text-decoration: none;
	background-image: url(<%= asset_path 'resume.png' %>);
	background-position: 10px 10px;
	background-repeat: no-repeat;
	padding-left: 24px;
}
#resume_body {
	color: #555;
}

.fixed-footer {
	bottom: 0;
	left: 0;
	right: 0;
	width: 100%;
	z-index: 214748364;
}
.fixed-footer .wrapper {
	min-height: 0;
}
 .license-status {
 	width: 200px;
 }
 .license-status-0 {
 	color: #FFCD03;
 }
 .license-status-1 {
 	color: #6ABD19;
 }
 
 .license-status-2 {
 	color: #CC0000;
}

/* Mobile Redirect */
#MobileRedirect {
	position: fixed;
	z-index: 214748364;
	left: 0px;
	right: 0px;
	top: 0px;
	bottom: 0px;
	padding: 0px;
	background: #6ABD19;
}
.app-icon, .app-stars, .iphone-app, .android-app {
	background: url(<%= asset_path 'redirect.png' %>) no-repeat;
}
.app-icon {
	display: block;
	width: 60px;
	height: 60px;
}

.app-stars {
	display: block;
	width: 92px;
	height: 16px;
	background-position: -60px 0;
}

.iphone-app {
	display: block;
	width: 180px;
	height: 357px;
	background-position: -196px -57px;
}

.android-app {
	display: block;
	width: 188px;
	height: 352px;
	background-position: 0px -62px;
}