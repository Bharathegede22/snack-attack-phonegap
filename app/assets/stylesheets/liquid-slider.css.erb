.reviews .liquid-slider {
	width:960px;
  height:120px;
  overflow:hidden;
}

.reviews .ls-wrapper {
  clear: both;
  overflow: visible;
  position: relative;
}

.reviews .ls-wrapper .liquid-slider {
  width:960px;
  height:120px;
  /* Do not edit below this line */
  overflow: hidden;
  position: relative;
  color:#666;
}

.reviews .ls-wrapper .panel-container {
  /* Do not edit below this line */
  position: relative;

}

.reviews .ls-wrapper .liquid-slider .panel {  
  width: 960px;
  height:120px;
  /* Do not edit below this line */
  display: block;
  float: left;
}

.reviews .ls-wrapper .liquid-slider .panel-wrapper {
  /* Do not edit below this line */
  position:relative;
}

.reviews .ls-wrapper .liquid-slider .panel-container {  
  height:120px;
  overflow:hidden;
}

.reviews .ls-wrapper .ls-nav-left-arrow, .reviews .ls-wrapper .ls-nav-right-arrow {
  cursor: pointer;
  width:40px;
  height:40px;
  position:absolute;
  top:10px;
}

.reviews .ls-wrapper .ls-nav-left-arrow a, .reviews .ls-wrapper .ls-nav-right-arrow a {
  display:block;
  width:40px;
  height:40px;
}

.reviews .ls-wrapper .ls-nav-left-arrow {
	background:url(<%= asset_path 'r.png' %>) no-repeat;
	background-position:-170px -160px;
  left:0px;
  z-index:30;
}

.reviews .ls-wrapper .ls-nav-right-arrow {
	right:0px;
	background:url(<%= asset_path 'r.png' %>) no-repeat;
	background-position:-210px -160px;
	z-index:30;
}

.reviews .ls-wrapper .ls-nav-left-arrow:hover {
  background-position:-170px -120px;
}

.reviews .ls-wrapper .ls-nav-left-arrow:active {
  background-position:-170px -160px;
}

.reviews .ls-wrapper .ls-nav-right-arrow:hover {
  background-position:-210px -120px;
}

.reviews .ls-wrapper .ls-nav-right-arrow:active {
  background-position:-210px -120px;
}
