.cars .liquid-slider {
	width:100%;
  height750px;
}

.cars .ls-wrapper {
  /* Do not edit below this line */
  clear: both;
  overflow: visible;
  position: relative;
  height:750px;
}

.cars .ls-wrapper .liquid-slider {
  width: 100%;
  height:750px;
  /* Do not edit below this line */
  float: left;
  overflow: hidden;
  position: relative;
}

.cars .ls-wrapper .panel-container {
  /* Do not edit below this line */
  position: relative;
}

.cars .ls-wrapper .liquid-slider .panel-container .fade {
  /* Do not edit below this line */
  position: absolute;
  top: 0; 
  left: 0;
  display: block;
}

.cars .ls-wrapper .liquid-slider .panel {  
  width: 100%;
  height:750px;
  /* Do not edit below this line */
  display: block;
  float: left;
}

.cars .ls-wrapper .liquid-slider .panel-wrapper {
  /* Do not edit below this line */
  position:relative;
}

.cars .ls-wrapper .ls-nav {
	position:absolute;
  bottom:300px;
  left:50%;
  z-index:20;
  width:960px;
  overflow:visible;
}

.cars .ls-wrapper .ls-nav a {
  color:#ffffff;
  text-decoration:none;
  background:#6ABD19;
  color:#ffffff;
  font-weight:bold;
}

.cars .ls-wrapper .ls-nav a:hover {
  background:#666666;
}

.cars .ls-wrapper .ls-nav .current {
  background:#323a45;
}

.cars .ls-wrapper .ls-nav ul {  
  padding:0;
  /* Do not edit below this line */
  clear: both;
  display: block;
  margin-left:-480px;
  overflow: hidden;
}

.cars .ls-wrapper .ls-nav ul li {
  /* Do not edit below this line */
  display: inline;
}

.cars .ls-wrapper .ls-nav ul li a {
  display: block;
  float: left;
  width:158px;
  text-align:center;
  font-size:16px;
  padding:10px 0 7px 0;
  margin:0 1px;
}

.cars .ls-wrapper .ls-nav-left-arrow, .cars .ls-wrapper .ls-nav-right-arrow {
  display:none;
}

