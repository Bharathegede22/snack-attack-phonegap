#header-new .navbar-nav *, .body-header-new .widget-hor * {
	/*color:#323a45;*/
	font-family: 'Helvetica', 'Arial', sans-serif;
	/*font-size:16px;*/
}

#header-new .topbar-link .caret {
	border-top-color:#ffffff;
	border-bottom-color:#ffffff;
}

#header-new .navbar-nav > li > a, #header-new .navbar-nav > li > div {
	line-height: 30px;
	padding: 6px 10px 6px 10px;
	margin-right: 2px;
}

#header-new .navbar-nav > li > a:hover, #header-new .navbar-nav > li > a:focus, #header-new .nav .open > a, #header-new .nav .open > a:hover, #header-new .nav .open > a:focus, #header-new .navbar-nav > li > .active  {
	/*background:#70757D url(<%= asset_path 'bg-line.png' %>);*/
	background: #EEE;
	border-radius: 5px;
	color:#323a45;
}
#header-new .nav .open > a, #header-new .nav .open > a:hover, #header-new .nav .open > a:focus {
	border-radius: 5px 5px 0 0;
}

#header-new .navbar-nav > li > a:hover .caret, #header-new .navbar-nav > li > .active .caret, #header-new .nav .open > a .caret, #header-new .nav .open > a:hover .caret, #header-new .nav .open > a:focus .caret {
	border-top-color:#323a45;
	border-bottom-color:#323a45;
}

#header-new .navbar-nav .dropdown-menu {
	/*background: #EEE;*/
	/*margin: 0;*/
	-webkit-border-radius: 0 0 5px 5px;
	-moz-border-radius: 0 0 5px 5px;
	border-radius: 0 0 5px 5px;
	border-color: #EEE;
	/*border-top: 0;*/
	/*border: 0;*/
	/*-moz-box-shadow: 0px 0px 0px 0px;*/
	/*box-shadow: 0px 0px 0px 0px;*/
}
#header-new .navbar-nav .dropdown-menu li a {
	font-size: 14px;
}

#topbar .dropdown-menu {
	color: #FFF;
	background: #495566;
	margin: 0;
	-webkit-border-radius: 0;
	-moz-border-radius: 0;
	border-radius: 0;
	border: 0;
	-moz-box-shadow: 0px 2px 2px 0px rgba(0,0,0,0.50);
	box-shadow: 0px 2px 2px 0px rgba(0,0,0,0.50);
}
#topbar .dropdown-menu * {
	color: #FFF;
}

#header-new .nav-buffer-1 {
	height:96px;
}
#header-new .nav-buffer-2 {
	height:137px;
}
#header-new .nav-buffer-3 {
	height:120px;
}
#header-new .nav-buffer-4 {
	height:161px;
}

#header-new .logo {
	background:url(<%= asset_path 'r.png' %>) no-repeat;
	height:50px;
	width:175px;
	display:block;
	margin:5px 10px 0 0;
	float:left;
}

#header-new .links {
	/*float:right;*/
	margin-top: 10px;
}

#city-selection > a {
	color: #FFF;
	text-decoration: none;
	height: 25px;
}
#city-selection > a:hover {
	background: #495566;
}
#city-selection > a.active {
	color: #323a45;
	background: #FFF;
}

#header-new .header-icon {
	background: url(<%= asset_path 'header-icons.png' %>) no-repeat;

}
#header-new .header-icon.facebook {
	width: 10px;
	height: 18px;
	background-position: -84px 0;
	padding: 2px;
}
#header-new .header-icon.facebook:hover {
	width: 10px;
	height: 18px;
	background-position: -84px -17px;
	padding: 2px;
}
#header-new .header-icon.twitter {
	width: 19px;
	height: 18px;
	background-position: -65px 0;
	padding: 2px;
}
#header-new .header-icon.twitter:hover {
	width: 19px;
	height: 18px;
	background-position: -65px -17px;
	padding: 2px;
}
#header-new .header-icon.iphone {
	width: 15px;
	height: 18px;
	background-position: 0 0;
	padding: 2px;
}
#header-new .header-icon.iphone:hover {
	width: 15px;
	height: 18px;
	background-position: 0 -17px;
	padding: 2px;
}
#header-new .header-icon.android {
	width: 16px;
	height: 18px;
	background-position: -16px 0;
	padding: 2px;
}
#header-new .header-icon.android:hover {
	width: 16px;
	height: 18px;
	background-position: -16px -17px;
	padding: 2px;
}
#header-new .header-icon.offers {
	width: 15px;
	height: 18px;
	background-position: -33px 0;
	padding: 3px;
}
#header-new .header-icon.offers:hover {
	width: 15px;
	height: 18px;
	background-position: -33px -17px;
	padding: 3px;
}
#header-new .header-icon.alerts {
	width: 15px;
	height: 18px;
	background-position: -49px 0;
	padding: 2px;
}
#header-new .header-icon.alerts.active {
	width: 15px;
	height: 18px;
	background-position: -49px -17px;
	padding: 2px;
}

#header-new .topbar-link, #header-new .topbar-link > * {
	color: #FFF;
	text-decoration: none;
}
#header-new .topbar-link:hover {
	color: #FFF;
	text-decoration: none;
	background: #495566;
}
#header-new .topbar-link:focus {
	color: #FFF;
	text-decoration: none;
	background: #495566;
}

#header-new .offers-link {
	/*border-bottom: 2px solid #6abd19;*/
	/*border-top: 2px solid #6abd19;*/
	background:url(<%= asset_path 'offers-border.png' %>);
	padding-top: 3px;
	padding-bottom: 2px;
}
#header-new .offers-link:hover {
	/*border-bottom: 2px solid #6abd19;*/
	/*border-top: 2px solid #6abd19;*/
	background-image:url(<%= asset_path 'offers-border.png' %>);
}

#header-new .search-header {
	background:url(<%= asset_path 'bg-line.png' %>) #fff;
	border-top: 1px solid #CCC;
	padding:5px 0 5px 0;
}

#alerts .dropdown-menu {
	width: 150px;
}
#alerts .dropdown-menu li a {
	white-space: normal;
	font-size: 11px;
	line-height: 14px;
}

.body-header-new .widget-hor .bg, .body-header-new .line-bg {
	background:url(<%= asset_path 'bg-line.png' %>) #fff;
}
@media screen and (max-width:768px) {
  .navbar-fixed-top {
    position: absolute;
  }
}